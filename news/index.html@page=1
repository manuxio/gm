<script type="text/javascript" defer="defer">
	jQuery(document).ready(function($) {	
		$('input[placeholder], textarea[placeholder]').placeholder();	
		$('.fancybox').fancybox();

		/*#menu-main .menu-item-depth-2.has-childs*/
		$('#menu-main .menu-item-depth-0.has-childs').hoverIntent({
		     over: menuHover,
		     out: menuLeave,
		     timeout: 800
		});
		
		function menuHover(e){
			var item = $(e.currentTarget);
			if(item.hasClass('menu-item-depth-0')) $(item).children('a.menu-link').addClass('active');
			//$(item).children('ul.sub-menu').stop(1,1).slideDown(100);
			$(item).children('ul.sub-menu').show();
		}

		function menuLeave(e){
			var item = $(e.currentTarget);
			/*$(item).children('ul.sub-menu').stop(1,1).slideUp(100, function() {
				if(item.hasClass('menu-item-depth-0')) $(item).children('a.menu-link').removeClass('active');
			});*/
			$(item).children('ul.sub-menu').hide();
			if(item.hasClass('menu-item-depth-0')) $(item).children('a.menu-link').removeClass('active');
		}
	});
</script>
